     <lb xml:id="fol_44r.8" n="8"/>MISERIAE CAUSA EX CONTRARIO EST <w part="I">INTEL</w>
     <lb xml:id="fol_44r.9" n="9"/><w part="F">LEGENDA</w> QUOD EST NON ADHERERE <app><rdg wit="#CSEL40">ADHAERERE</rdg></app> <choice><abbr>D&#772;O&#772;</abbr><expan>D<ex>E</ex>O</expan></choice> <choice><abbr>QUA &#772;</abbr><expan>QUA<ex>M</ex></expan></choice>
     <lb xml:id="fol_44r.10" n="10"/>OB REM SI CUM QUAERITUR QUARE ILLI <w part="I">BEA</w>
