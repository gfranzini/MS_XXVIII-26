     <lb xml:id="fol_43v.7" n="7"/>DE IPSIS ANGELIS UIDEO ESSE DICENDA
     <lb xml:id="fol_43v.8" n="8"/><choice><abbr>QUIB&#183;</abbr><expan>QUIB<ex>US</ex></expan></choice> DEMONSTRETUR QUANTUM A <w part="I">NO</w>
     <lb xml:id="fol_43v.9" n="9"/><w part="F">BIS</w> POTEST QUAM NON INCONUENIENS
     <lb xml:id="fol_43v.10" n="10"/>NEQUE INCONGRUA DICATUR ESSE <choice><abbr><w part="I">HO</w>
     <lb xml:id="fol_43v.11" n="11"/><w part="F">MINIB&#183;</w></abbr><expan>HOMINIB<ex>US</ex></expan></choice> <choice><abbr>ANGELISQ&#183;</abbr><expan>ANGELISQ<ex>UE</ex></expan></choice> SOCIETAS UT NON <w part="I">QUAT</w>
     <lb xml:id="fol_43v.12" n="12"/><w part="F">TUOR</w> DUAE SCILICET ANGELORUM <choice><abbr><w part="I">TOTIDE &#772;</w>
     <lb xml:id="fol_43v.13" n="13"/><w part="F">QUE</w></abbr><expan>TOTIDE<ex>M</ex>QUE</expan></choice> HOMINUM SED DUAE POTIUS <w part="I">CIUITA</w>
     <lb xml:id="fol_43v.14" n="14"/><w part="F">TES</w> HOC EST SOCIETATES MERITO ESSE <w part="I">DI</w>
     <lb xml:id="fol_43v.15" n="15"/><w part="F">CANTUR</w> UNA IN BONIS ALTERA IN MALIS
     <lb xml:id="fol_43v.16" n="16"/>NON SOLUM ANGELIS UERUM ETIAM <choice><abbr><w part="I">HO</w>
     <lb xml:id="fol_43v.17" n="17"/><w part="F">MINIB&#183;</w></abbr><expan>HOMINIB<ex>US</ex></expan></choice> CONSTITUTAE ANGELORUM <w part="I">BONO</w>
