   <div n="fol_43v" type="edition">
    <ab>
     <lb xml:id="fol_43v.1" n="1"/>ANTEQUAM DE INSTITUTIONE HOMINIS
     <lb xml:id="fol_43v.2" n="2"/><note type="scholion" place="margin">&#8278; <choice><abbr>duaru &#772;</abbr><expan>duaru<ex>m</ex></expan></choice> <choice><abbr>ciuitatu &#772;</abbr><expan>ciuitatu<ex>m</ex></expan></choice></note> DICAM UBI <add place="above">&#8278;</add> DUARUM CIUITATUM <choice><abbr><w part="I">QUA &#772;</w>
     <lb xml:id="fol_43v.3" n="3"/><w part="F">TUM</w></abbr><expan>QUA<ex>N</ex>TUM</expan></choice> AD RATIONALIUM MORTALIUM <w part="I">GE</w>
     <lb xml:id="fol_43v.4" n="4"/><w part="F">NUS</w> ATTINET <app><rdg wit="#CSEL40">ADTINET</rdg></app> APPAREBIT EXORTUS SICUT
