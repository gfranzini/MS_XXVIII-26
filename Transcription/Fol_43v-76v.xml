     <lb xml:id="fol_44r.12" n="12"/><w part="F">HERENT</w> <app><rdg wit="#CSEL40">ADHAERENT</rdg></app>; ET CUM QUAERITUR QUUR ISTI <choice><sic>S<del rend="strikethrough">U</del><hi rend="ligature">NT</hi></sic><corr>S<add place="above">I</add><hi rend="ligature">NT</hi></corr></choice>
     <lb xml:id="fol_44r.13" n="13"/>MISERI RECTE RESPONDETUR QUIA NO<add>N</add>&#772;
     <lb xml:id="fol_44r.14" n="14"/>ADHERENT <app><rdg wit="#CSEL40">ADHAERENT</rdg></app> <choice><abbr>D&#772;O&#772;</abbr><expan>D<ex>E</ex>O</expan></choice> NON EST CREATURAE <w part="I">RATIO</w> <note type="scholion" place="margin"><choice><abbr>R&#772;</abbr><expan>R<ex>equire</ex></expan></choice></note>
     <lb xml:id="fol_44r.15" n="15"/><w part="F">NALIS</w> UEL INTELLECTUALIS BONUM QUO
     <lb xml:id="fol_44r.16" n="16"/>BEATA SIT NISI DEUS ITA QUAMUIS NON
     <lb xml:id="fol_44r.17" n="17"/>OMNIS BEATA POSSIT ESSE CREATURA <choice><abbr>NEQ&#183;</abbr><expan>NEQ<ex>UE</ex></expan></choice>
     <lb xml:id="fol_44r.18" n="18"/>ENIM HOC MUNUS ADIPISCUNTUR AUT
     <lb xml:id="fol_44r.19" n="19"/>CAPIUNT FAERAE <app><rdg wit="#CSEL">FERAE</rdg></app> LIGNA SAXA ET SI QUID <w part="I">HU</w>
     <lb xml:id="fol_44r.20" n="20"/><w part="F">IUS</w> MODI EST EA TAMEN QUAE POTEST NO<add>N</add>&#772;
     <lb xml:id="fol_44r.21" n="21"/>EX SE IPSA POTEST QUIA EX NIHILO CREATA
     <lb xml:id="fol_44r.22" n="22"/>EST SED <choice><sic>E<del rend="strikethrough">T</del></sic><corr>E<add place="above">X</add></corr></choice> ILLO A QUO CREATA EST HOC ENIM
     <lb xml:id="fol_44r.23" n="23"/>ADEPTO BEATA QUO AMISSO MISERA EST
     <lb xml:id="fol_44r.24" n="24"/>ILLE UERO QUI NON ALIO SED SE IPSO BONO <note type="scholion" place="margin"><choice><abbr>R&#772;</abbr><expan>R<ex>equire</ex></expan></choice></note>
     <lb xml:id="fol_44r.25" n="25"/>BEATUS EST IDEO MISER NON POTEST ESSE
     <lb xml:id="fol_44r.26" n="26"/>QUIA NON SE POTEST AMITTERE DICIM<hi rend="ligature">US</hi>&#183;
     <lb xml:id="fol_44r.27" n="27"/>ITAQUE IMMUTABILE <app><rdg wit="#CSEL40">INCOMMUTABILE</rdg></app> BONUM NON ESSE
