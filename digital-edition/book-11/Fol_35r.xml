   <div n="Folio 35r" type="edition">
    <ab>
     <lb xml:id="Folio35r.1" n="1"/>RERUM CORPORALIUM NON QUIA <w part="I">SEN</w>
     <lb xml:id="Folio35r.2" n="2"/><w part="F">TIUNT</w> SED QUIA <choice><sic>SENTI<del rend="strikethrough">A</del>NTUR</sic><corr>SENTI<add place="above">U</add>NTUR</corr></choice> SENSIBILIA
