     <lb xml:id="Folio35r.5" n="5"/>GIGNUNT UERUM TAMEN ET HAEC OM<supplied reason="lost">NIA</supplied> <app><rdg wit="#CSEL40">ET OMNIA</rdg></app>
     <lb xml:id="Folio35r.6" n="6"/>CORPORALIA LATENTES IN NATURA CAUSAS
     <lb xml:id="Folio35r.7" n="7"/>HABENT SED FORMAS SUAS <choice><abbr>QUIB&#183;</abbr><expan>QUIB<ex>US</ex></expan></choice> MUNDI <supplied reason="lost"><w part="I">HU</w></supplied>
     <lb xml:id="Folio35r.8" n="8"/><w part="F">IUS</w> UISIBILIS STRUCTURA FORMOSA EST <w part="I">SEN</w>
     <lb xml:id="Folio35r.9" n="9"/><w part="F">TIENDAS</w> <choice><abbr>SENSIB&#183;</abbr><expan>SENSIB<ex>US</ex></expan></choice> PRAEBENT UT <add place="above">PRO</add> EO QUOD
     <lb xml:id="Folio35r.10" n="10"/>NOSSE NON POSSUNT QUASI <w part="I">INNO<supplied reason="lost">TESCE</supplied></w>
     <lb xml:id="Folio35r.11" n="11"/><w part="F">RE</w> UELLE UIDETUR <app><rdg wit="#CSEL40">UIDEANTUR</rdg></app> SED NOS EA SENSU
     <lb xml:id="Folio35r.12" n="12"/>CORPORIS ITA CAPIMUS UT DE HIS NON SENSU
     <lb xml:id="Folio35r.13" n="13"/>CORPORIS IUDICEMUS HABEMUS ENIM
     <lb xml:id="Folio35r.14" n="14"/>ALIUM INTERIORIS HOMINIS SENSUM ISTO
     <lb xml:id="Folio35r.15" n="15"/>LONGE PRAESTANTIOREM QUO IUSTA ET <w part="I">INIUS</w>
     <lb xml:id="Folio35r.16" n="16"/><w part="F">TA</w> <supplied reason="lost">SENTIEMUS IUSTA</supplied> PER INTELLEGIBILEM
     <lb xml:id="Folio35r.17" n="17"/>SPECIEM INIUSTA PER EIUS PRIUATIONEM
     <lb xml:id="Folio35r.18" n="18"/>AD HUIUS SENSUS OFFICIUM NON ACIES <w part="I">PU</w>
     <lb xml:id="Folio35r.19" n="19"/><w part="F">PULAE</w> NON FORAMEN AURICULAE NON <w part="I">SPI</w>
     <lb xml:id="Folio35r.20" n="20"/><w part="F">RAMENTA</w> NARIUM NON GUSTUS FA<add place="above">U</add>CIUM
     <lb xml:id="Folio35r.21" n="21"/>NON ULLUS CORPOREUS TACTUS ACCEDIT
     <lb xml:id="Folio35r.22" n="22"/>IBI ME ET ESSE ET IN HOC NOSSE CERTUS SUM
     <lb xml:id="Folio35r.23" n="23"/>ET HAEC AMO <choice><abbr>ATQ&#183;</abbr><expan>ATQ<ex>UE</ex></expan></choice> <app><rdg wit="#CSEL40">ADQUE</rdg></app> AMARE ME SIMILITER <w part="I">CER</w>
     <lb xml:id="Folio35r.24" n="24"/><w part="F">TUS</w> SUUM <app><rdg wit="#CSEL40">SUM</rdg></app> SED DE <choice><abbr>DUOB&#183;</abbr><expan>DUOB<ex>US</ex></expan></choice> ILLIS ESSENTIA
     <lb xml:id="Folio35r.25" n="25"/>SCILICET ET NOTITIA QUANTUM AMENTUR
     <lb xml:id="Folio35r.26" n="26"/>IN NOBIS ET QUEM AMMODUM <app><rdg wit="#CSEL40">AD MODUM</rdg></app> ETIAM IN
     <lb xml:id="Folio35r.27" n="27"/>CETERIS REBUS QUAE INFRA SUNT EORUM
     <lb xml:id="Folio35r.28" n="28"/>REPPERIATUR <app><rdg wit="#CSEL40">REPERIATUR</rdg></app> ETSI DIFFERENS QUAEDAM
     <lb xml:id="Folio35r.29" n="29"/>TAMEN SIMILITUDO QUANTUM SUSCEPIT
