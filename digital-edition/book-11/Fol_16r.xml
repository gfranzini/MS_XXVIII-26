   <div n="Folio 16r" type="edition">
    <ab>
     <lb xml:id="Folio16r.1" n="1"/>ISTAM UANITATEM ILLA SCRIBTURA <w part="I">PA</w>
     <lb xml:id="Folio16r.2" n="2"/><w part="F">RIS</w> AUCTORITATIS UBI <choice><abbr>D&#772;S&#772;</abbr><expan>D<ex>EU</ex>S</expan></choice> DICIT, QUANDO
     <lb xml:id="Folio16r.3" n="3"/>FACTA SUNT SIDERA LAUDAUERUNT ME <note type="scholion" place="margin">R&#772; laudauerim ... angeli mei uoce...<!--illegible--></note>
     <lb xml:id="Folio16r.4" n="4"/>UOCE MAGNA OMNES ANGELI MEI IAM
     <lb xml:id="Folio16r.5" n="5"/>ERGO ERANT ANGELI QUANDO FACTA SU<hi rend="ligature">NT</hi>
     <lb xml:id="Folio16r.6" n="6"/>SIDERA FACTA SUNT AUTEM QUARTO DIE
     <lb xml:id="Folio16r.7" n="7"/>NUMQUIDNAM ERGO DIE TERTIO FACTOS
     <lb xml:id="Folio16r.8" n="8"/>ESSE DICEMUS? ABSIT &#183; IN PROMTUM <app><rdg wit="#CSEL40">IN PROMTU</rdg></app> EST
     <lb xml:id="Folio16r.9" n="9"/>ENIM QUID ILLO DIE FACTUM SIT AB AQU<hi rend="ligature">IS</hi>
     <lb xml:id="Folio16r.10" n="10"/>UTIQUE TERRA DISCRETA EST ET <w part="I">DISTINC</w>
     <lb xml:id="Folio16r.11" n="11"/><w part="F">TA</w> <app><rdg wit="#CSEL40">DISTINCTAS</rdg></app> SUI GENERIS SPECIES DUO ISTA <choice><abbr><w part="I">ELEME &#772;</w></abbr><expan>ELEME<ex>N</ex>
     <lb xml:id="Folio16r.12" n="12"/><w part="F">TA</w></expan></choice> SUMSERUNT ET PRODUXIT TERRA
     <lb xml:id="Folio16r.13" n="13"/>QUIDQUID EI RADICITUS INHAERET
     <lb xml:id="Folio16r.14" n="14"/>NUMQUIDNAM SECUNDUM <app><rdg wit="#CSEL40">SECUNDO</rdg></app>? NE HOC <w part="I">QUI</w>
     <lb xml:id="Folio16r.15" n="15"/><w part="F">DEM</w>; TUNC ENIM FIRMAMENTUM <sic>EST</sic>
     <lb xml:id="Folio16r.16" n="16"/>FACTUM EST SUPER <app><rdg wit="#CSEL40">INTER</rdg></app> AQUAS SUPERIORES <w part="I">IN</w>
     <lb xml:id="Folio16r.17" n="17"/><w part="F">FERIORES</w> <app><rdg wit="#CSEL40">ET INFERIORES</rdg></app> CAELUMQUE APPELLATUM EST
     <lb xml:id="Folio16r.18" n="18"/>IN QUO FIRMAMENTO QUARTO DIE FACTA
     <lb xml:id="Folio16r.19" n="19"/>SUNT SIDERA NIMIRUM ERGO SI AD 
     <lb xml:id="Folio16r.20" n="20"/>ISTORUM DIERUM OPERA <choice><abbr>D&#772;I&#772;</abbr><expan>D<ex>E</ex>I</expan></choice> &#183; PERTINE<hi rend="ligature">NT</hi> <note type="scholion" place="margin">R&#772; de ingens</note>
     <lb xml:id="Folio16r.21" n="21"/><supplied reason="lost">ANGELI</supplied> IPSI SUNT ILLA LUX QUI DIE <app><rdg wit="#CSEL40">QUAE DIEI</rdg></app> <choice><abbr>NOME &#772;</abbr><expan>NOME<ex>N</ex></expan></choice>
     <lb xml:id="Folio16r.22" n="22"/>ACCEPIT CUIUS UNITAS UT <w part="I">COMMENDA</w>
     <lb xml:id="Folio16r.23" n="23"/><w part="F">RET<supplied reason="lost">UR</supplied></w> NON EST DICTUS DIES PRIMUS <supplied reason="lost">SED</supplied>
     <lb xml:id="Folio16r.24" n="24"/>DIES UNUS NEC ALIUS EST DIES <w part="F"><supplied reason="lost">SECUN</supplied></w>
     <lb xml:id="Folio16r.25" n="25"/><w part="F">DUS</w> AUT TERTIUS AUT CETERI SED IDEM <w part="I">IP</w>
     <lb xml:id="Folio16r.26" n="26"/><w part="F">SE</w> UN<del rend="strikethrough">I</del>US AD IMPLENDUM <app><rdg wit="#CSEL40">INPLENDUM</rdg></app> SENARIUM
     <lb xml:id="Folio16r.27" n="27"/>UEL SEPTENARIUM NUMERUM <w part="I">REPETI</w>
     <lb xml:id="Folio16r.28" n="28"/><w part="F">TUS</w> EST PROPTER SEPTENARIAM <app><rdg wit="#CSEL40">SENARIAM UEL SEPTENARIAM</rdg></app> <w part="I">COGNI</w>
     <lb xml:id="Folio16r.29" n="29"/><w part="F">TIONEM</w> SENARIAM SCILICET <supplied reason="lost">OPERUM</supplied>
     <lb xml:id="Folio16r.30" n="30"/>QUAE FECIT <choice><abbr>D&#772;S&#772;</abbr><expan>D<ex>EU</ex>S</expan></choice> ET SEPTIMAM <app><rdg wit="#CSEL40">SEPTENARIAM</rdg></app> QUIETIS <choice><abbr>D&#772;I&#772;</abbr><expan>D<ex>E</ex>I</expan></choice>
    </ab>  
