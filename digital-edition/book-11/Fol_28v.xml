     <lb xml:id="Folio28v.14" n="14"/>TOTAM SED QUOD EIUS NON POTUERIT AB
     <lb xml:id="Folio28v.15" n="15"/>ILLA INQUINATIONE PURGARI TEGMEN AC
     <lb xml:id="Folio28v.16" n="16"/>UINCULUM FUTURUM HOSTIS UICTI ET <w part="I">IN</w>
     <lb xml:id="Folio28v.17" n="17"/><w part="F">CLUSI</w> SIC AUTEM MANICHEI NON <w part="I">DESIPE</w>
     <lb xml:id="Folio28v.18" n="18"/><w part="F">RENT</w> UEL POTIUS INSANIRENT SI <choice><abbr>D&#772;I&#772;</abbr><expan>D<ex>E</ex>I</expan></choice> <w part="I">NATU</w>
     <lb xml:id="Folio28v.19" n="19"/><w part="F">RAM</w> SICUTI EST INCOMMUTABILEM <choice><abbr>ATQ&#183;</abbr><expan>ATQ<ex>UE</ex></expan></choice>
     <lb xml:id="Folio28v.20" n="20"/>OMNINO INCORRUPTIBILEM CREDERE<hi rend="ligature">NT</hi>
     <lb xml:id="Folio28v.21" n="21"/>CUI NOCERE NULLA RES POSSIT &#183; ANIMAM
     <lb xml:id="Folio28v.22" n="22"/>UERO QUAE UOLUNTATE MUTARI IN <w part="I">DETE</w>
     <lb xml:id="Folio28v.23" n="23"/><w part="F">RIUS</w> ET PECCATO CORRUMPI POTUIT <choice><abbr>ATQ&#183;</abbr><expan>ATQ<ex>UE</ex></expan></choice> ITA
     <lb xml:id="Folio28v.24" n="24"/>INCOMMUTABILIS UERITATIS LUCE <w part="I">PRIUA</w>
     <lb xml:id="Folio28v.25" n="25"/><w part="F">RI</w> NON DEI PARTEM NEC EIUS NATURAE
     <lb xml:id="Folio28v.26" n="26"/>QUAE <choice><abbr>D&#772;I&#772;</abbr><expan>D<ex>E</ex>I</expan></choice> EST SED AB ILLO CONDITAM <w part="I">LON</w>
     <lb xml:id="Folio28v.27" n="27"/><w part="F">GE</w> IN PAREM CONDITORI <choice><abbr>XP&#772;I&#772;ANA</abbr><expan>CHRI<ex>STI</ex>ANA</expan></choice> SANITATE
     <lb xml:id="Folio28v.28" n="28"/><note type="scholion" subtype="chapter" n="23">XXIII</note> SENTIRENT SED MULTO EST <choice><abbr>MIRANDU &#772;</abbr><expan>MIRANDU<ex>M</ex></expan></choice>
