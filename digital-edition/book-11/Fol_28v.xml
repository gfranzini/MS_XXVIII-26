   <div n="Folio 28v" type="edition">
    <ab>
     <lb xml:id="Folio28v.1" n="1"/>MALAM PUTANT ESSE NATURAM SUO
     <lb xml:id="Folio28v.2" n="2"/>QUODAM CONTRARIO EXORTAM <choice><abbr><w part="I">PROPA</w>
