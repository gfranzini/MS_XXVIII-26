     <lb xml:id="Folio28v.10" n="10"/><w part="F">CENDO</w> <choice><abbr>SUPERANDOQ&#183;</abbr><expan>SUPERANDOQ<ex>UE</ex></expan></choice> MISCUISSE QUAM
     <lb xml:id="Folio28v.11" n="11"/>TURPISSIME POLLUTAM ET CRUDELISSIME
     <lb xml:id="Folio28v.12" n="12"/>CAPTIUATAM ET OPPRAESSAM LABORE <w part="I">MAG</w>
