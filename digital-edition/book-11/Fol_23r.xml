   <div n="Folio 23r" type="edition">
    <ab>
     <lb xml:id="Folio23r.1" n="1"/><note type="scholion" place="top"><choice><abbr>LIB&#772;.</abbr><expan>LIB<ex>ER</ex></expan></choice> X&#772;I&#772;</note> A QUO EST OMNIS MODUS OMNIS <w part="I">SPE</w>
     <lb xml:id="Folio23r.2" n="2"/><w part="F">CIES</w> OMNIS ORDO SINE <choice><abbr>QUIB&#183;</abbr><expan>QUIB<ex>US</ex></expan></choice> NIHIL <choice><abbr>RERU &#772;</abbr><expan>RERU<ex>M</ex></expan></choice>
     <lb xml:id="Folio23r.3" n="3"/>INUENIRI UEL COGITARI POTEST QUANTO
     <lb xml:id="Folio23r.4" n="4"/>MAGIS ANGELICA CREATURA QUAE OMNIA
     <lb xml:id="Folio23r.5" n="5"/>CETERA QUAE <choice><abbr>D&#772;S&#772;</abbr><expan>D<ex>EU</ex>S</expan></choice> CONDIDIT NATURAE <w part="I">DIGNI</w>
     <lb xml:id="Folio23r.6" n="6"/><w part="F">TATE</w> PRAECEDIT; <note type="scholion" subtype="chapter" n="16" place="margin">XVI</note> JN HIS ENIM QUAE <w part="I">QUO</w>
     <lb xml:id="Folio23r.7" n="7"/><w part="F">QUO</w> MODO SUNT ET NON SUNT QUOD <choice><abbr>D&#772;S&#772;</abbr><expan>D<ex>EU</ex>S</expan></choice> EST
     <lb xml:id="Folio23r.8" n="8"/>A QUO FACTA SUNT PRAEPONUNTUR <w part="I">UIUEN</w>
     <lb xml:id="Folio23r.9" n="9"/><w part="F">TJA</w> NON <choice><abbr>UIUENTIB&#183;</abbr><expan>UIUENTIB<ex>US</ex></expan></choice> SICUT EA QUAE HABENT
     <lb xml:id="Folio23r.10" n="10"/>UIM GIGNENDI UEL ETIAM APPETENDI <app><rdg wit="#CSEL40">ADPETENDI</rdg></app> HIS
     <lb xml:id="Folio23r.11" n="11"/>QUAE ISTO MOTU CARENT ET IN HIS QUAE
     <lb xml:id="Folio23r.12" n="12"/>UIUUNT PRAEPONUNTUR SENTIENTIA
     <lb xml:id="Folio23r.13" n="13"/>NON <choice><abbr>SENTIENTIB&#183;</abbr><expan>SENTIENTIB<ex>US</ex></expan></choice> SICUT <choice><abbr>ARBORIB&#183;</abbr><expan>ARBORIB<ex>US</ex></expan></choice> <w part="I">ANIMA</w>
     <lb xml:id="Folio23r.14" n="14"/><w part="F">LIA</w> ET IN HIS QUAE SENTIUNT <choice><abbr><w part="I">PRAEPONU &#772;</w>
     <lb xml:id="Folio23r.15" n="15"/><w part="F">TUR</w></abbr><expan>PRAEPONU<ex>N</ex>TUR</expan></choice> INTELLEGENTIA NON <choice><abbr>INTELLEGENTIB&#183;</abbr><expan>INTELLEGENTIB<ex>US</ex></expan></choice>
     <lb xml:id="Folio23r.16" n="16"/>SICUT HOMINES <choice><abbr>PECORIB&#183;</abbr><expan>PECORIB<ex>US</ex></expan></choice> ET IN HIS QUAE
     <lb xml:id="Folio23r.17" n="17"/>INTELLEGUNT PRAEPONUNTUR <w part="I">IMMOR</w>
     <lb xml:id="Folio23r.18" n="18"/><w part="F">TALIA</w> <app><rdg wit="#CSEL40">INMORTALIA</rdg></app> <choice><abbr>MORTALIB&#183;</abbr><expan>MORTALIB<ex>US</ex></expan></choice> SICUT ANGELI <choice><abbr>HOMINIB&#183;</abbr><expan>HOMINIB<ex>US</ex></expan></choice>
     <lb xml:id="Folio23r.19" n="19"/>SED ITA PRAEPONUNTUR NATURAE ORDINE
     <lb xml:id="Folio23r.20" n="20"/>EST AUTEM ALIUS <choice><abbr>ATQ&#183;</abbr><expan>ATQ<ex>UE</ex></expan></choice> <app><rdg wit="#CSEL40">ADQUE</rdg></app> ALIUS PRO SUO <w part="I">CUIUS</w>
     <lb xml:id="Folio23r.21" n="21"/><w part="F">QUE</w> USU ESTIMATIONIS <app><rdg wit="#CSEL40">AESTIMATIONIS</rdg></app> MODUS QUO FIT
     <lb xml:id="Folio23r.22" n="22"/>UT QUAEDAM SENSU CARENTIA <choice><abbr>QUIBUSDA &#772;</abbr><expan>QUIBUSDA<ex>M</ex></expan></choice>
     <lb xml:id="Folio23r.23" n="23"/>SENTIENTIBUS PRAEPONAMUS JN <choice><abbr>TANTU &#772;</abbr><expan>TANTU<ex>M</ex></expan></choice> <note type="scholion" place="margin">R&#772; <choice><abbr>qd&#772;</abbr><expan>q<ex>uo</ex>d</expan></choice> si potestas <choice><abbr>ess&#38;</abbr><expan>ess<ex>et</ex></expan></choice></note>
     <lb xml:id="Folio23r.24" n="24"/>UT SI POTESTAS ESSET EA PRORSUS DE <w part="I">NATU</w>
     <lb xml:id="Folio23r.25" n="25"/><w part="F">RA</w> RERUM AUFERRE UELLEMUS SIUE <choice><abbr>QUE &#772;</abbr><expan>QUE<ex>M</ex></expan></choice>
     <lb xml:id="Folio23r.26" n="26"/>IN EA LOCUM HABEANT IGNORANTES SIUE
     <lb xml:id="Folio23r.27" n="27"/>ETIAM <app><rdg wit="#CSEL40">ETIAMSI</rdg></app> SCIAMUS NOSTRIS EA COMMODIS
     <lb xml:id="Folio23r.28" n="28"/>POSTPONENTES : QUIS ENIM NON DOMUI <app><rdg wit="#CSEL40">DOMI</rdg></app> <note type="scholion" place="margin">R&#772; mures</note>
     <lb xml:id="Folio23r.29" n="29"/>SUAE PANEM HABERE QUAM MURES <sic><w part="I">NU</w>
     <lb xml:id="Folio23r.30" n="30"/><w part="F">MOS</w></sic> <app><rdg wit="#CSEL40">NUMMOS</rdg></app> QUAM PULICES MALIT? SED QUID <w part="I">MI</w>
    </ab>  
