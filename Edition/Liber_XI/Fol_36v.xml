   <div n="Folio 36v" type="edition">
    <ab>
     <lb xml:id="Folio36v.1" n="1"/><w part="F">REM</w> IBI AMARE NOSTRUM NON <w part="I">HA</w>
     <lb xml:id="Folio36v.2" n="2"/><w part="F">BEBIT</w> OFFENSIONEM NUNC AUTEM TRIA
     <lb xml:id="Folio36v.3" n="3"/>ISTA NOSTRA QUAMUIS CERTA TENEAM<hi rend="ligature">US</hi>
     <lb xml:id="Folio36v.4" n="4"/>NEC ALIIS EA CREDAMUS <choice><abbr>TESTIB&#183;</abbr><expan>TESTIB<ex>US</ex></expan></choice> SED NOS
     <lb xml:id="Folio36v.5" n="5"/>IPSI PRAESENTIA SENTIAMUS <choice><abbr>ATQ&#183;</abbr><expan>ATQ<ex>UE</ex></expan></choice> <app><rdg wit="#CSEL40">ADQUE</rdg></app> <w part="I">INTERIO</w>
     <lb xml:id="Folio36v.6" n="6"/><w part="F">RE</w> UERACISSIMO CERNAMUS ASPECTU <w part="I">TA</w>
     <lb xml:id="Folio36v.7" n="7"/><w part="F">MEN</w> QUAMDIU FUTURA UEL UTRUM <choice><abbr><w part="I">NU &#772;</w>
     <lb xml:id="Folio36v.8" n="8"/><w part="F">QUAM</w></abbr><expan>NU<ex>M</ex>QUAM</expan></choice> DEFUTURA ET QUO SI MALE QUO <choice><abbr>AUTE &#772;</abbr><expan>AUTE<ex>M</ex></expan></choice>
     <lb xml:id="Folio36v.9" n="9"/>SI BENE AGANTUR PERUENTURA SINT <w part="I">QUO</w>
     <lb xml:id="Folio36v.10" n="10"/><w part="F">NIAM</w> PER NOS IPSOS NOSSE NON POSSUM<hi rend="ligature">US</hi>
     <lb xml:id="Folio36v.11" n="11"/>ALIOS HINC TESTES UEL QUAERIMUS UEL
     <lb xml:id="Folio36v.12" n="12"/>HABEMUS DE QUORUM FIDE QUUR <w part="I">NUL</w>
     <lb xml:id="Folio36v.13" n="13"/><w part="F">LA</w> DEBEAT ESSE DUBITATIO NON EST ISTE SED
     <lb xml:id="Folio36v.14" n="14"/>POSTERIOR ERIT DILIGENTIUS <w part="I">DISSEREN</w>
     <lb xml:id="Folio36v.15" n="15"/><w part="F">DI</w> LOCUS IN HOC AUTEM LIBRO DE <w part="I">CIUI</w>
     <lb xml:id="Folio36v.16" n="16"/><w part="F">TATE</w> <choice><abbr>D&#772;I&#772;</abbr><expan>D<ex>E</ex>I</expan></choice>&#183; QUAE NON PEREGRINATUR IN <w part="I">HU</w>
     <lb xml:id="Folio36v.17" n="17"/><w part="F">IUS</w> UITAE <del rend="expunction">IM</del>MORTALITATE SED <w part="I">IMMOR</w>
     <lb xml:id="Folio36v.18" n="18"/><w part="F">TALIS</w> <app><rdg wit="#CSEL40">INMORTALIS</rdg></app> SEMPER IN CAELIS EST ID EST DE <w part="I">AN</w>
     <lb xml:id="Folio36v.19" n="19"/><w part="F">GELIS</w> <choice><abbr>S&#772;C&#772;I&#772;S&#772;</abbr><expan>S<ex>AN</ex>C<ex>T</ex>IS</expan></choice> <choice><abbr>D&#772;O&#772;</abbr><expan>D<ex>E</ex>O</expan></choice> <choice><abbr>COHERENTIB&#183;</abbr><expan>COHERENTIB<ex>US</ex></expan></choice> <app><rdg wit="#CSEL40">COHAERENTIBUS</rdg></app> QUI NEC <w part="I">FUE</w>
     <lb xml:id="Folio36v.20" n="20"/><w part="F">RANT</w> <app><rdg wit="#CSEL40">FUERUNT</rdg></app> UMQUAM NEC FUTURI SUNT <w part="I">DESER</w>
     <lb xml:id="Folio36v.21" n="21"/><w part="F">TORES</w> INTER QUOS ET ILLOS QUI <choice><abbr>AETERNA &#772;</abbr><expan>AETERNA<ex>M</ex></expan></choice>
     <lb xml:id="Folio36v.22" n="22"/>LUCEM DESERENTES TENEBRAE FACTI SU<hi rend="ligature">NT</hi>
     <lb xml:id="Folio36v.23" n="23"/><choice><abbr>D&#772;M&#772;</abbr><expan>D<ex>EU</ex>M</expan></choice> PRIMITUS DIUISISSE IAM DIXIMUS <w part="I">IL</w>
     <lb xml:id="Folio36v.24" n="24"/><w part="F">LO</w> ADIUUANTE QUOD COEPIMUS UT <w part="I">POSSU</w>
     <lb xml:id="Folio36v.25" n="25"/><w part="F">MUS</w> EXPLICEMUS &#183; <note type="scholion" subtype="chapter" n="29" place="margin">XXIX</note> ILLI QUIPPE ANGELI <w part="I">SANC</w>
     <lb xml:id="Folio36v.26" n="26"/><w part="F">TI</w> NON PER UERBA SONANTIA <choice><abbr>D&#772;M&#772;</abbr><expan>D<ex>EU</ex>M</expan></choice> DISCUNT
     <lb xml:id="Folio36v.27" n="27"/>SED PER IPSAM PRAESENTIAM <w part="I">IMMUTABI</w>
     <lb xml:id="Folio36v.28" n="28"/><w part="F">LIS</w> <app><rdg wit="#CSEL40">INMUTABILIS</rdg></app> UERITAS <app><rdg wit="#CSEL40">UERITATIS</rdg></app> HOC EST UERBUM EIUS <w part="I">UNIGE</w>
     <lb xml:id="Folio36v.29" n="29"/><w part="F">NITUM</w> ET IPSUM UERBUM ET PATREM ET
     <lb xml:id="Folio36v.30" n="30"/>EORUM <choice><abbr>S&#772;P&#772;M&#772;</abbr><expan>SP<ex>IRITU</ex>M</expan></choice> <choice><abbr>S&#772;C&#772;M&#772;</abbr><expan>S<ex>AN</ex>C<ex>TU</ex>M</expan></choice> <choice><abbr>EAMQ&#183;</abbr><expan>EAMQ<ex>UE</ex></expan></choice> ESSE <w part="I">INSEPARABI</w>
    </ab> 
