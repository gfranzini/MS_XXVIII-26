     <lb xml:id="Folio37v.17" n="17"/><w part="F">RANTUR</w> NON QUIA <choice><abbr>D&#772;O&#772;</abbr><expan>D<ex>E</ex>O</expan></choice> FUERIT <w part="I">NECESSA</w>
     <lb xml:id="Folio37v.18" n="18"/><w part="F">RIA</w> MORA TEMPORUM QUASI QUI NON <w part="I">PO</w>
     <lb xml:id="Folio37v.19" n="19"/><w part="F">TUERIT</w> CREARE OMNIA <choice><abbr>SIMULQ&#183;</abbr><expan>SIMULQ<ex>UE</ex></expan></choice> <app><rdg wit="#CSEL40">SIMUL QUAE DEINCEPS</rdg></app> CONGR<hi rend="ligature">UI</hi>S
     <lb xml:id="Folio37v.20" n="20"/><choice><abbr>MOTIB&#183;</abbr><expan>MOTIB<ex>US</ex></expan></choice> PERGERENT <app><rdg wit="#CSEL40">PERAGERENT</rdg></app> TEMPORA SED QUIA
