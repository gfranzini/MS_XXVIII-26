     <lb xml:id="Folio37v.13" n="13"/><w part="F">NE</w> LUCESCIT IN <choice><abbr>MENTIB&#183;</abbr><expan>MENTIB<ex>US</ex></expan></choice> <w part="I">CONTEMPLAN</w>
