   <div n="Folio 15v" type="edition">
    <ab>
     <lb xml:id="Folio15v.1" n="1"/>QUAE PER SEX DIES CONSUMMATA <w part="I">NAR</w>
     <lb xml:id="Folio15v.2" n="2"/><w part="F">RANTUR</w> QUO MODO ANGELI <w part="I">PRAETER</w> 
     <lb xml:id="Folio15v.3" n="3"/><w part="F">MITTERENTUR</w> TAMQUAM NON ESSE<hi rend="ligature">NT</hi>
     <lb xml:id="Folio15v.4" n="4"/>IN <choice><abbr>OPERIB&#183;</abbr><expan>OPERIB<ex>US</ex></expan></choice> <choice><abbr>D&#772;I&#772;</abbr><expan>D<ex>E</ex>I</expan></choice> A <choice><abbr>QUIB&#183;</abbr><expan>QUIB<ex>US</ex></expan></choice> IN DIE SEPTIMO <w part="I">RE</w>
     <lb xml:id="Folio15v.5" n="5"/><w part="F">QUIEUIT</w> OPUS AUTEM <choice><abbr>DI&#772;</abbr><expan>D<ex>E</ex>I</expan></choice> &#183; ESSE <w part="I">ANGE</w>
     <lb xml:id="Folio15v.6" n="6"/><w part="F">LOS</w> HIC QUIDEM ETSI NON <w part="I">PRAETERMIS</w>
     <lb xml:id="Folio15v.7" n="7"/><w part="F">SUM</w> NON TAMEN EUIDENTER <w part="I">EXPRAES</w>
     <lb xml:id="Folio15v.8" n="8"/><w part="F">SUM</w> <app><rdg wit="#CSEL40">EXPRESSUM</rdg></app>EST SED ALIBI HOC SANCTA <w part="I">SCRIBTU</w>
     <lb xml:id="Folio15v.9" n="9"/><w part="F">RA</w> <app><rdg wit="#CSEL40">SCRIPTURA</rdg></app> CLARISSIMA UOCE TESTATUR NAM ET
     <lb xml:id="Folio15v.10" n="10"/>IN <sic>YMNO</sic> <app><rdg wit="#CSEL40">HYMNO</rdg></app> TRIUM IN CAMINO UIRORUM
     <lb xml:id="Folio15v.11" n="11"/>CUM PRAEDICTUM ESSET BENEDICITE <w part="I">OM</w>
     <lb xml:id="Folio15v.12" n="12"/><w part="F">NIA</w> OPERA <choice><abbr>D&#772;N&#772;I&#772;</abbr><expan>D<ex>OMI</ex>NI</expan></choice> &#183; <choice><abbr>D&#772;N&#772;M&#772;</abbr><expan>D<ex>OMI</ex>N<ex>U</ex>M</expan></choice> &#183; IN EXECUTIONE
     <lb xml:id="Folio15v.13" n="13"/>EORUNDEM OPERUM ETIAM ANGELI <w part="I">NO</w>
     <lb xml:id="Folio15v.14" n="14"/><w part="F">MINATI</w> SUNT ET IN PSALMO CANITUR
     <lb xml:id="Folio15v.15" n="15"/>LAUDATE <choice><abbr>D&#772;N&#772;M&#772;</abbr><expan>D<ex>OMI</ex>N<ex>U</ex>M</expan></choice> &#183; DE CAELIS LAUDATE EUM
     <lb xml:id="Folio15v.16" n="16"/>IN EXCELSIS LAUDATE EUM OMNES <w part="I">ANGE</w>
     <lb xml:id="Folio15v.17" n="17"/><w part="F">LI</w> EIUS LAUDATE EUM OMNES UIRTUTES 
     <lb xml:id="Folio15v.18" n="18"/>EIUS LAUDATE EUM SOL ET LUNA LAUDATE
     <lb xml:id="Folio15v.19" n="19"/>EUM OMNES STELLAE ET LUMEN <w part="I">LAUDA</w>
     <lb xml:id="Folio15v.20" n="20"/><w part="F">TE</w> EUM CAELI CAELORUM ET AQUAE QUAE
     <lb xml:id="Folio15v.21" n="21"/>SUPER CAELO <app><rdg wit="#CSEL40">CAELOS</rdg></app> SUNT LAUDENT NOMEN <choice><abbr>D&#772;N&#772;I&#772;</abbr><expan>D<ex>OMI</ex>NI</expan></choice>
     <lb xml:id="Folio15v.22" n="22"/>QUONIAM IPSE DIXIT ET FACTA SUNT IPSE 
     <lb xml:id="Folio15v.23" n="23"/>MANDAUIT ET <sic>CREA</sic> <app><rdg wit="#CSEL40">CREATA</rdg></app> SUNT ETIAM HIC <w part="I">APER</w>
     <lb xml:id="Folio15v.24" n="24"/><w part="F">TISSIME</w> A <choice><abbr>D&#772;O&#772;</abbr><expan>D<ex>E</ex>O</expan></choice> FACTOS ESSE ANGELOS <w part="I">DIUI</w>
     <lb xml:id="Folio15v.25" n="25"/><w part="F">NITUS</w> DICTUM EST CUM EIS INTER <w part="I">CAELES</w>
     <lb xml:id="Folio15v.26" n="26"/><w part="F">TIA</w> <app><rdg wit="#CSEL40">INTER CETERA CAELESTIA</rdg></app> <supplied reason="lost">COMMEMORATIS INFERTUR AD</supplied> OMNIA
     <lb xml:id="Folio15v.27" n="27"/><supplied reason="lost">IPSE DIXIT ET FACTA SUNT QUIS PORRO <w part="I">AU</w></supplied>
     <lb xml:id="Folio15v.28" n="28"/><supplied reason="lost"><w part="F">DEBIT</w> OPINARI POST OMNIA ISTA QUAE SEX</supplied>
     <lb xml:id="Folio15v.29" n="29"/><supplied reason="lost">DIEBUS ENUMERATA SUNT ANGELOS FACTOS</supplied>
     <lb xml:id="Folio15v.30" n="30"/><supplied reason="lost">SED ETSI QUISQUAM ITA DESIPIT REDARGUIT</supplied>
    </ab>  
