  <body>
   <head>
    <title xml:id="book">Liber XI</title>
   </head>
   
   <div n="Folio 29v" type="edition">
    <ab>
     <lb xml:id="Folio29v.1" n="1"/><w part="F">DI</w> MUNDI INTELLEGI UOLUIT NISI UT    
     <lb xml:id="Folio29v.2" n="2"/>BONA FIERENT A BONO <choice><abbr>D&#772;O&#772;</abbr><expan>D<ex>E</ex>O</expan></choice> UBI SI NEMO
     <lb xml:id="Folio29v.3" n="3"/>PECCASSET TANTUMMODO NATURALI<del rend="strikethrough">S</del> <app><rdg wit="#CSEL40">NATURIS</rdg></app> <w part="I">BO</w>
     <lb xml:id="Folio29v.4" n="4"/><note type="scholion" place="margin">plenus boni</note> <w part="F">NI&#778;</w> <app><rdg wit="#CSEL40">BONIS</rdg></app> ESSET MUNDUS OR<del rend="strikethrough">DI</del>NATUS ET PLEN<hi rend="ligature">US</hi>
     <lb xml:id="Folio29v.5" n="5"/>ET QUIA PECCATUM EST NON IDEO CUNCTA
     <lb xml:id="Folio29v.6" n="6"/>SUNT IMPLETA <app><rdg wit="#CSEL40">INPLETA</rdg></app> PECCATIS CUM BONORUM
     <lb xml:id="Folio29v.7" n="7"/>LONGE MAIOR NUMERUS IN CAELESTIBUS
     <lb xml:id="Folio29v.8" n="8"/>SUAE NATURAE ORDINEM SERUET NEC <w part="I">MA</w>
     <lb xml:id="Folio29v.9" n="9"/><w part="F">LA</w> UOLUNTAS QUIA NATURAE ORDINEM
     <lb xml:id="Folio29v.10" n="10"/>SERUARE NOLUIT IDEO IUSTI <del rend="expunction">DE</del> <choice><abbr>D&#772;I&#772;</abbr><expan>D<ex>E</ex>I</expan></choice> &#183; LEGES
     <lb xml:id="Folio29v.11" n="11"/>OMNIA BENE ORDINANTIS EFFUGIT <w part="I">QUO</w>
     <lb xml:id="Folio29v.12" n="12"/><w part="F">NIAM</w> SICUT PICTURA CUM COLORE NIGRO
     <lb xml:id="Folio29v.13" n="13"/>LOCO SUO POSITO ITA UNIUERSITAS <choice><abbr>RERU &#772;</abbr><expan>RERU<ex>M</ex></expan></choice>
     <lb xml:id="Folio29v.14" n="14"/>SI QUIS POSSIT INTUERI ETIAM CUM <choice><abbr><w part="I">PECCA</w>
     <lb xml:id="Folio29v.15" n="15"/><w part="F">TORIB&#183;</w></abbr><expan>PECCATORIB<ex>US</ex></expan></choice> PULCHRA EST QUAMIS PER SE IPSOS
     <lb xml:id="Folio29v.16" n="16"/>CONSIDERATOS SUA DEFORMITAS TURPET
     <lb xml:id="Folio29v.17" n="17"/>DEINDE UIDERE DEBUIT ORIGINES ET <w part="I">QUI</w>
     <lb xml:id="Folio29v.18" n="18"/><w part="F">CUMQUE</w> ITA SAPIUNT SI HAEC OPINIO <w part="I">UE</w>
     <lb xml:id="Folio29v.19" n="19"/><w part="F">RA</w> ESSET MUNDUM IDEO FACTUM UT <w part="I">ANI</w>
     <lb xml:id="Folio29v.20" n="20"/><w part="F">MAE</w> PRO MERITIS PECCATORUM <choice><abbr>SUORU &#772;</abbr><expan>SUORU<ex>M</ex></expan></choice>
     <lb xml:id="Folio29v.21" n="21"/>TAMQUAM ERGASTULA <choice><abbr>QUIB&#183;</abbr><expan>QUIB<ex>US</ex></expan></choice> POENALITER
     <lb xml:id="Folio29v.22" n="22"/>INCLUDERENTUR CORPORA ACCIPERENT 
     <lb xml:id="Folio29v.23" n="23"/>SUPERIORA ET LEUIORA QUAE MINUS <w part="I">INFE</w>
     <lb xml:id="Folio29v.24" n="24"/><w part="F">RIORA</w> UERO ET GRAUIORA QUAE AMPLIUS
     <lb xml:id="Folio29v.25" n="25"/>PECCAUERUNT DAEMONES <choice><abbr>QUIB&#183;</abbr><expan>QUIB<ex>US</ex></expan></choice> DETERI<hi rend="ligature">US</hi>
     <lb xml:id="Folio29v.26" n="26"/>NIHIL EST TERRENA <sic>CORPORORA</sic> <app><rdg wit="#CSEL40">CORPORA</rdg></app> <choice><abbr>QUIB&#183;</abbr><expan>QUIB<ex>US</ex></expan></choice> <w part="I">IN</w>
     <lb xml:id="Folio29v.27" n="27"/><w part="F">FERIUS</w> ET GRAUIUS NIHIL EST POTIUS <choice><abbr>QUA &#772;</abbr><expan>QUA<ex>M</ex></expan></choice>
     <lb xml:id="Folio29v.28" n="28"/>HOMINES ETIAM BONOS HABERE DEBUISSE
     <lb xml:id="Folio29v.29" n="29"/>NUNC UERO UT INTELLEGEREMUS <w part="I">ANIMA</w>
     <lb xml:id="Folio29v.30" n="30"/><w part="F">RUM</w> MERITA NON <choice><abbr>QUALITATIB&#183;</abbr><expan>QUALITATIB<ex>US</ex></expan></choice> <choice><abbr>CORPORU &#772;</abbr><expan>CORPORU<ex>M</ex></expan></choice> 
    </ab>     
