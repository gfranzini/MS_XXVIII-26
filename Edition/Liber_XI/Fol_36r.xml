   <div n="Folio 36r" type="edition">
    <ab>
     <lb xml:id="Folio36r.1" n="1"/>ULLO QUIDEM SENSU <choice><abbr>ATQ&#183;</abbr><expan>ATQ<ex>UE</ex></expan></choice> <app><rdg wit="#CSEL40">ADQUE</rdg></app> UITA NON <w part="I">TA</w>
     <lb xml:id="Folio36r.2" n="2"/><w part="F">MEN</w> NOBIS DEESSET QUASI QUIDAM <w part="I">NOS</w>
     <lb xml:id="Folio36r.3" n="3"/><w part="F">TRORUM</w> LOCORUM <choice><abbr>ATQ&#183;</abbr><expan>ATQ<ex>UE</ex></expan></choice> <app><rdg wit="#CSEL40">ADQUE</rdg></app> ORDINIS APPETIT<hi rend="ligature">US</hi> <app><rdg wit="#CSEL40">ADPETITUS</rdg></app>
     <lb xml:id="Folio36r.4" n="4"/>NAM UELUT AMORES CORPORUM <w part="I">MOMEN</w>
     <lb xml:id="Folio36r.5" n="5"/><w part="F">TA</w> SUNT PONDERUM SIUE DEORSUM <w part="I">GRA</w>
     <lb xml:id="Folio36r.6" n="6"/><w part="F">UITATE</w> SIUE SURSUM LEUITATE NITANT<hi rend="ligature">UR</hi>
     <lb xml:id="Folio36r.7" n="7"/>ITA ENIM CORPUS PONDERE SICUT ANIMUS
     <lb xml:id="Folio36r.8" n="8"/>AMORE FERTUR QUOCUMQUE FERTUR <w part="I">QUO</w>
     <lb xml:id="Folio36r.9" n="9"/><w part="F">NIAM</w> IGITUR HOMINES SUMUS AD NOSTRI
     <lb xml:id="Folio36r.10" n="10"/>CREATORIS IMAGINEM CREATI CUIUS EST
     <lb xml:id="Folio36r.11" n="11"/>UERA AETERNITAS AETERNA UERITAS <w part="I">AETER</w>
     <lb xml:id="Folio36r.12" n="12"/><w part="F">NA</w> ET UERA CARITAS ESTQUE IPSE AETERNA 
     <lb xml:id="Folio36r.13" n="13"/>ET UERA ET CARA TRINITAS <choice><abbr>NEQ&#183;</abbr><expan>NEQ<ex>UE</ex></expan></choice> CONFUSA <choice><abbr>NEQ&#183;</abbr><expan>NEQ<ex>UE</ex></expan></choice>
     <lb xml:id="Folio36r.14" n="14"/>SEPARATA IN HIS QUIDEM <choice><abbr>REB&#183;</abbr><expan>REB<ex>US</ex></expan></choice> QUAE INFRA
     <lb xml:id="Folio36r.15" n="15"/>NOS SUNT QUONIAM ET IPSA NEC ALIQUO
     <lb xml:id="Folio36r.16" n="16"/>MODO ESSENT NEC ALIQUA SPECIE <w part="I">CONTINE</w>
     <lb xml:id="Folio36r.17" n="17"/><w part="F">RENTUR</w> NEC ALIQUEM ORDINEM UEL <supplied reason="lost"><w part="I">AP</w></supplied>
     <lb xml:id="Folio36r.18" n="18"/><w part="F">PETERENT</w> <app><rdg wit="#CSEL40">ADPETERENT</rdg></app> UEL TENERENT NISI AB ILLO FACTA
     <lb xml:id="Folio36r.19" n="19"/>ESSENT QUI SUMME EST QUI SUMME <w part="I">SAPI</w>
     <lb xml:id="Folio36r.20" n="20"/><w part="F">ENS</w> EST QUI SUMME BONUS EST <choice><abbr>TAMQUA &#772;</abbr><expan>TAMQUA<ex>M</ex></expan></choice>
     <lb xml:id="Folio36r.21" n="21"/>PER OMNIA QUAE FECIT MIRABILI <w part="I">STABILI</w>
     <lb xml:id="Folio36r.22" n="22"/><w part="F">TATE</w> CURRENTIS <app><rdg wit="#CSEL40">CURRENTES</rdg></app> QUASI QUAEDAM EIUS <w part="I">ALI</w>
     <lb xml:id="Folio36r.23" n="23"/><w part="F">BI</w> MAGI<add place="above">U</add>S ALIBI MINUS INPRAESSA <app><rdg wit="#CSEL40">IMPRESSA</rdg></app> <w part="I">UESTI</w>
     <lb xml:id="Folio36r.24" n="24"/><w part="F">GIA</w> COLLIGAMUS IN NOBIS AUTEM IPSIS EI<hi rend="ligature">US</hi>
     <lb xml:id="Folio36r.25" n="25"/>IMAGINEM CONTUENTES TAMQUAM <w part="I">MI</w>
     <lb xml:id="Folio36r.26" n="26"/><w part="F">NOR</w> ILLE EUANGELICUS FILIUS AD NOSMET
     <lb xml:id="Folio36r.27" n="27"/>IPSOS REUERSI SURGAMUS ET AD ILLUM <w part="I">EA</w>
     <lb xml:id="Folio36r.28" n="28"/><w part="F">MUS</w> <app><rdg wit="#CSEL40">REDEAMUS</rdg></app> A QUO PECCANDO RECESSERAMUS
     <lb xml:id="Folio36r.29" n="29"/>IBI ESSE NOSTRUM NON HABEBIT MORTEM
     <lb xml:id="Folio36r.30" n="30"/>IBI NOSSE NOSTRUM NON HABEBIT <w part="I">ERRO</w>
