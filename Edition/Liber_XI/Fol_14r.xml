     <lb xml:id="Folio14r.3" n="3"/>SONABILI ET TEMPORALI ET REQUIES <app><rdg wit="#CSEL40">SED REQUIES</rdg></app> <choice><abbr>D&#772;I&#772;</abbr><expan>D<ex>E</ex>I</expan></choice> <w part="I">RE</w>
     <lb xml:id="Folio14r.4" n="4"/><w part="F">QUIEM</w> SIGNIFICAT EORUM QUI <w part="I">REQUIES</w>
