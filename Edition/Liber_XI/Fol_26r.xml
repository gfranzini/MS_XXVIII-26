     <lb xml:id="Folio26r.10" n="10"/><w part="F">UIT</w> NOCTEM NON HOC LOCO ADDITUM EST
     <lb xml:id="Folio26r.11" n="11"/>ET UIDIT <choice><abbr>D&#772;S&#772;</abbr><expan>D<ex>EU</ex>S</expan></choice> QUIA BONUM EST ET NE <w part="I">UTRUM</w>
     <lb xml:id="Folio26r.12" n="12"/><w part="F">QUE</w> APPELLARETUR BONUM CUM ESSET <w part="I">HO</w>
