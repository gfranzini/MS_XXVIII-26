     <lb xml:id="Folio26r.23" n="23"/>UT NIHIL EORUM FIERET SI EI FUISSET <w part="I">INCOG</w>
     <lb xml:id="Folio26r.24" n="24"/><w part="F">NITUM</w> DUM ERGO UIDET QUIA <choice><abbr>BONU &#772;</abbr><expan>BONU<ex>M</ex></expan></choice>
     <lb xml:id="Folio26r.25" n="25"/>EST QUOD NISI UIDISSET ANTEQUAM FIERET
     <lb xml:id="Folio26r.26" n="26"/>NON <choice><abbr>UTIQ&#183;</abbr><expan>UTIQ<ex>UE</ex></expan></choice> FIERET DOCET BONUM ESSE <choice><abbr>NO &#772;</abbr><expan>NO<ex>N</ex></expan></choice>
     <lb xml:id="Folio26r.27" n="27"/>DISCIT ET PLATO QUIDEM PLUS AUSUS EST
     <lb xml:id="Folio26r.28" n="28"/>DICERE ELATUM ESSE SCILICET <choice><abbr>D&#772;M&#772;</abbr><expan>D<ex>EU</ex>M</expan></choice> GAUDIO
