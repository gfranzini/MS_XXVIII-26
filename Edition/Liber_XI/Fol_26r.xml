     <lb xml:id="Folio26r.14" n="14"/>NATURA; ET IDEO SOLA IBI LUX PLACUIT <w part="I">CONDI</w>
     <lb xml:id="Folio26r.15" n="15"/><w part="F">TORI</w> TENEBRAE AUTEM ANGELICAE ETSI <w part="I">FUE</w>
     <lb xml:id="Folio26r.16" n="16"/><w part="F">RANT</w> ORDINANDAE NON TAMEN FUERANT
     <lb xml:id="Folio26r.17" n="17"/>APPROBANDAE <app><rdg wit="#CSEL40">ADPROBANDAE</rdg></app> QUID EST ENIM ALIUD <w part="I">INTEL</w>
     <lb xml:id="Folio26r.18" n="18"/><w part="F">LEGENDUM</w> IN EO QUOD PER OMNIA <w part="I">DICI</w>
     <lb xml:id="Folio26r.19" n="19"/><w part="F">TUR</w> UIDIT <choice><abbr>D&#772;S&#772;</abbr><expan>D<ex>EU</ex>S</expan></choice> &#183; QUIA BONUM EST NISI OPERIS
     <lb xml:id="Folio26r.20" n="20"/>APPROBATIO <app><rdg wit="#CSEL40">ADPROBATIO</rdg></app> SECUNDUM ARTEM FACTI QUAE
     <lb xml:id="Folio26r.21" n="21"/>SAPIENTIA <choice><abbr>D&#772;I&#772;</abbr><expan>D<ex>E</ex>I</expan></choice> &#183; EST <choice><abbr>D&#772;S&#772;</abbr><expan>D<ex>EU</ex>S</expan></choice> AUTEM <choice><abbr>USQ&#183;</abbr><expan>USQ<ex>UE</ex></expan></choice> ADEO
