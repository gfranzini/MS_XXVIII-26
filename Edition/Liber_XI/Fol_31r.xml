   <div n="Folio 31r" type="edition">
    <ab>
     <note type="scholion" place="top"><choice><abbr>L&#772;I&#772;B&#772;</abbr><expan>LIB<ex>ER</ex></expan></choice> X&#772;I&#772;</note>
     <lb xml:id="Folio31r.1" n="1"/>AUDEO TEMERARIAM PRAECIPITARE
     <lb xml:id="Folio31r.2" n="2"/>SENTENTIAM UERUM TAMEN <choice><abbr>AMBORU &#772;</abbr><expan>AMBORU<ex>M</ex></expan></choice>
     <lb xml:id="Folio31r.3" n="3"/><choice><sic>E<del rend="strikethrough">U</del>M</sic><corr>E<add place="above">A</add>M</corr></choice> DICERE SANCTITATEM FACILIUS AUS<hi rend="ligature">US</hi>
     <lb xml:id="Folio31r.4" n="4"/>FUERO NON AMBORUM QUASJ <w part="I">QUALITA</w>
     <lb xml:id="Folio31r.5" n="5"/><w part="F">TEM</w> SED IPSUM QUOQUE SUBSTANTIAM
     <lb xml:id="Folio31r.6" n="6"/>ET TERTIAM IN TRINITATE PERSONAM <note type="scholion" place="margin">: <choice><abbr>tertia &#772;</abbr><expan>tertia<ex>m</ex></expan></choice> in trinitate <choice><abbr>persona &#772;</abbr><expan>persona<ex>m</ex></expan></choice></note>
     <lb xml:id="Folio31r.7" n="7"/>AD HOC ENIM ME PROBABILIUS DUCIT QUOD
     <lb xml:id="Folio31r.8" n="8"/>CUM SIT ET PATER <note type="scholion" place="margin">: <choice><abbr>R&#772;</abbr><expan>R<ex>equire</ex></expan></choice> <choice><abbr>pat &#772;</abbr><expan>pat<ex>er</ex></expan></choice> <choice><abbr>s&#772;p&#772;s&#772;</abbr><expan>sp<ex>iritu</ex>s</expan></choice> <choice><abbr>&#38;</abbr><expan><ex>et</ex></expan></choice> filius <choice><abbr>s&#772;p&#772;s&#772;</abbr><expan>sp<ex>iritu</ex>s</expan></choice></note> <choice><abbr>S&#772;P&#772;S&#772;</abbr><expan>SP<ex>IRITU</ex>S</expan></choice> ET FILIUS <choice><abbr>S&#772;P&#772;S&#772;</abbr><expan>SP<ex>IRITU</ex>S</expan></choice> ET PATER
     <lb xml:id="Folio31r.9" n="9"/><choice><abbr>S&#772;C&#772;S&#772;</abbr><expan>S<ex>AN</ex>C<ex>TU</ex>S</expan></choice> ET FILIUS <choice><abbr>S&#772;C&#772;S&#772;</abbr><expan>S<ex>AN</ex>C<ex>TU</ex>S</expan></choice> PROPRIAE <app><rdg wit="#CSEL40">PROPRIE</rdg></app> TAMEN IPSE
     <lb xml:id="Folio31r.10" n="10"/>UOCATUR <choice><abbr>S&#772;P&#772;S&#772;</abbr><expan>SP<ex>IRITU</ex>S</expan></choice> <choice><abbr>S&#772;C&#772;S&#772;</abbr><expan>S<ex>AN</ex>C<ex>TU</ex>S</expan></choice> TAMQUAM SANCTITAS
     <lb xml:id="Folio31r.11" n="11"/>SUBSTANTIALIS ET CONSUBSTANTIALIS <w part="I">AM</w>
     <lb xml:id="Folio31r.12" n="12"/><w part="F">BORUM</w> SED SI NIHIL EST ALIUD BONITAS
     <lb xml:id="Folio31r.13" n="13"/>DIUINA QUAM SANCTITAS PROFECTO ET ILLA
     <lb xml:id="Folio31r.14" n="14"/>DILIGENTIA <app><rdg wit="#CSEL40">INTELLEGENTIA</rdg></app> RATIONIS EST NON <w part="I">PRAESUM</w>
     <lb xml:id="Folio31r.15" n="15"/><w part="F">TIONIS</w> AUDACIA UT IN <choice><abbr>OPERIB&#183;</abbr><expan>OPERIB<ex>US</ex></expan></choice> <choice><abbr>D&#772;I&#772;</abbr><expan>D<ex>E</ex>I</expan></choice> SECRETO  
     <lb xml:id="Folio31r.16" n="16"/>QUODAM LOQUENDI MODO QUO NOSTRA
     <lb xml:id="Folio31r.17" n="17"/>EXERCEATUR INTENTIO EADEM NOBIS <w part="I">IN</w>
     <lb xml:id="Folio31r.18" n="18"/><w part="F">SINUATA</w> INTELLEGATUR TRINITAS <w part="I">UNAQUAE</w>
     <lb xml:id="Folio31r.19" n="19"/><w part="F">QUE</w> TRINITAS <choice><abbr>UNAMQUAMQ&#183;</abbr><expan>UNAMQUAMQ<ex>UE</ex></expan></choice> <app><rdg wit="#CSEL40">INTELLEGATUR TRINITAS UNAMQUAMQUE</rdg></app> CR<add place="above">E</add>ATURAM <note type="scholion" place="margin">: <choice><abbr>qs</abbr><expan>q<ex>ui</ex>s</expan></choice> fecerit</note>
     <lb xml:id="Folio31r.20" n="20"/>QUIS FECERIT &#183; PER QUID FECERIT &#183; PROPTER
     <lb xml:id="Folio31r.21" n="21"/>QUID FECERIT; PATER QUIPPE <w part="I">INTELLEGI</w>
     <lb xml:id="Folio31r.22" n="22"/><w part="F">TUR</w> UERBI QUI DIXIT UT FIAT QUOD AUTEM
     <lb xml:id="Folio31r.23" n="23"/>ILLO DICENTE FACTUM EST PROCUL DUBIO
     <lb xml:id="Folio31r.24" n="24"/>PER UERBUM FACTUM EST; IN EO UERO
     <lb xml:id="Folio31r.25" n="25"/>QUOD DICITUR UIDIT <choice><abbr>D&#772;S&#772;</abbr><expan>D<ex>EU</ex>S</expan></choice> QUIA BONUM EST
     <lb xml:id="Folio31r.26" n="26"/>SATIS SIGNIFICATUR <choice><abbr>D&#772;M&#772;</abbr><expan>D<ex>EU</ex>M</expan></choice> NULLA <w part="I">NECESSI</w>
     <lb xml:id="Folio31r.27" n="27"/><w part="F">TATE</w> NULLA SUAE CUIUSQUAM UTILITATIS
     <lb xml:id="Folio31r.28" n="28"/>INDIGENTIA SED SOLA BONITATE FECISSE
     <lb xml:id="Folio31r.29" n="29"/>QUOD FACTUM EST ID EST QUIA BONUM EST
     <lb xml:id="Folio31r.30" n="30"/>QUOD IDEO POSTEA QUAM FACTUM EST
    </ab>     
