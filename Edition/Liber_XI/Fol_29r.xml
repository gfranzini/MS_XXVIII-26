     <lb xml:id="Folio29r.4" n="4"/><w part="F">RUNT</w> TAMEN ISTAM CAUSAM FABRICAN<supplied reason="lost">DI</supplied>
     <lb xml:id="Folio29r.5" n="5"/>MUNDI TAM BONAM AC SIMPLICEM BENE
     <lb xml:id="Folio29r.6" n="6"/>AC SIMPLICITER CREDERE UT <choice><abbr>D&#772;S&#772;</abbr><expan>D<ex>EU</ex>S</expan></choice> BONUS <choice><abbr><w part="I">CO &#772;</w>
