     <lb xml:id="Folio29r.11" n="11"/><w part="F">TAS</w> A <choice><abbr>D&#772;O&#772;</abbr><expan>D<ex>E</ex>O</expan></choice> PECCASSE A CONDITORE <choice><abbr><w part="I"><supplied reason="lost">RE</supplied>CEDE &#772;</w>
     <lb xml:id="Folio29r.12" n="12"/><w part="F">DO</w></abbr><expan>RECEDE<ex>N</ex>DO</expan></choice> ET DIUERSIS <choice><abbr>PROGRESSIB&#183;</abbr><expan>PROGRESSIB<ex>US</ex></expan></choice> PRO <w part="I">DIUERSI</w>
     <lb xml:id="Folio29r.13" n="13"/><w part="F">TATE</w> PECCATORUM A CAELIS <choice><abbr>USQ&#183;</abbr><expan>USQ<ex>UE</ex></expan></choice> AD <supplied reason="lost">TERRAS</supplied>
     <lb xml:id="Folio29r.14" n="14"/>DIUERSA CORPORA QUASI UINCULA <w part="I">MERU</w> 
     <lb xml:id="Folio29r.15" n="15"/><w part="F">ISSE</w> ET HUNC ESSE MUNDUM EAMQUE
     <lb xml:id="Folio29r.16" n="16"/>CAUSAM MUNDI FUISSE FACIENDI NON
     <lb xml:id="Folio29r.17" n="17"/>UT CONDERENTUR BONA SED UT MALA <w part="I">CO</w>
     <lb xml:id="Folio29r.18" n="18"/><w part="F">HIBERENTUR</w> HINC ORIGENES IURE <w part="I">CUL</w> <note type="scholion" place="margin">hic dicit de origine...</note>
     <lb xml:id="Folio29r.19" n="19"/><w part="F">PATUR</w> IN LIBRIS ENIM QUOS APPELLAT PERI 
     <lb xml:id="Folio29r.20" n="20"/>&#8278; ARCHON <app><note>Translitteration of the Greek περὶ ἀρχῶν</note></app> ID EST DE PRINCIPIIS HOC SENSIT
     <lb xml:id="Folio29r.21" n="21"/>HOC SCRIBSIT <app><rdg wit="#CSEL40">SCRIPSIT</rdg></app> UBI PLUS QUAM DICI POTEST <note type="scholion" place="margin">&#8278; peri archon</note>
     <lb xml:id="Folio29r.22" n="22"/>MIROR HOMINEM IN ECCLESIASTICIS <w part="I">LIT</w>
     <lb xml:id="Folio29r.23" n="23"/><w part="F">TERIS</w> TAM DOCTUM ET EXERCITATUM <choice><abbr>NO &#772;</abbr><expan>NO<ex>N</ex></expan></choice>
