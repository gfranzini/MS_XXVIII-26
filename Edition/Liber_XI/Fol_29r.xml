    <ab>
     <lb xml:id="Folio29r.1" n="1"/>RERUM ESSE PRINCIPIUM ULLAMQUE
     <lb xml:id="Folio29r.2" n="2"/>NATURAM QUAE NON EST QUOD <choice><abbr>D&#772;S&#772;</abbr><expan>D<ex>EU</ex>S</expan></choice> EST <add place="above">NISI</add> AB
