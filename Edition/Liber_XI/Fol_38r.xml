     <lb xml:id="Folio38r.12" n="12"/>SIT DICI NON POTEST UNUM AUTEM POTEST
     <lb xml:id="Folio38r.13" n="13"/>NAM DECIMA PARS EIUS EST HABET ET <choice><abbr><w part="I">QUI &#772;</w>
     <lb xml:id="Folio38r.14" n="14"/><w part="F">TAM</w></abbr><expan>QUI<ex>N</ex>TAM</expan></choice> QUOD SUNT DUO HABET ET <choice><abbr>DIMIDIA &#772;</abbr><expan>DIMIDIA<ex>M</ex></expan></choice>
     <lb xml:id="Folio38r.15" n="15"/>QUOD SUNT QUINQUE SED HAE TRES <w part="I">PAR</w>
     <lb xml:id="Folio38r.16" n="16"/><w part="F">TES</w> EIUS DECIMA ET QUINTA ET DIMIDIA
     <lb xml:id="Folio38r.17" n="17"/>ID EST UNUM ET DUO ET QUINQUE SIMUL
     <lb xml:id="Folio38r.18" n="18"/>DUCTAE NON CONPLENT DECEM SUNT <choice><abbr>ENI &#772;</abbr><expan>ENI<ex>M</ex></expan></choice>
     <lb xml:id="Folio38r.19" n="19"/>OCTO<add>;</add> DUODENARI <app><rdg wit="#CSEL40">DUODENARII</rdg></app> UERO NUMERI PARTES <note type="scholion" place="margin">duodenarius</note>
     <lb xml:id="Folio38r.20" n="20"/>IN SUMMAM DUCTAE TRANSEUNT <choice><abbr>EU &#772;</abbr><expan>EU<ex>M</ex></expan></choice>
     <lb xml:id="Folio38r.21" n="21"/>HABET ENIM DUODECIMAM QUOD EST
     <lb xml:id="Folio38r.22" n="22"/>UNUM HABET SEXTAM QUAE SUNT <add place="above">DUO HABET</add> <w part="I">QUA<add place="left">R</add>T</w> <add place="right">TAM</add>
     <lb xml:id="Folio38r.23" n="23"/><add place="above">QUAE SUNT TRIA HABET TERTIAM QUAE SUNT <w part="I">QUAT</w></add> <w part="F">TUOR</w> HABET <app><rdg wit="#CSEL40">HABET ET</rdg></app> DIMIDIAM QUAE SUNT
     <lb xml:id="Folio38r.24" n="24"/>SEX UNUM AUTEM ET DUO ET TRIA ET <w part="I">QUAT</w>
     <lb xml:id="Folio38r.25" n="25"/><w part="F">TUOR</w> ET SEX NON DUODECUM SED AMPLI<hi rend="ligature">US</hi>
     <lb xml:id="Folio38r.26" n="26"/>ID EST SEDECIM FIUNT HOC BREUITER
     <lb xml:id="Folio38r.27" n="27"/>COMMEMORANDUM PUTAUI AD <w part="I">COM</w>
     <lb xml:id="Folio38r.28" n="28"/><w part="F">MENDANDAM</w> SENARI <app><rdg wit="#CSEL40">SENARII</rdg></app> NUMERI <w part="I">PER</w>
