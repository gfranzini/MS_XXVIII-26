     <lb xml:id="Folio38r.9" n="9"/><w part="F">GE</w></abbr><expan>LO<ex>N</ex>GE</expan></choice> SUNT A TOTA SUMMA EIUS QUOD EST
     <lb xml:id="Folio38r.10" n="10"/>NOUEM <choice><abbr>ITEMQ&#183;</abbr><expan>ITEMQ<ex>UE</ex></expan></choice> IN DENARIO <w part="I">QUATER</w> <note type="scholion" place="margin">denarius</note>
