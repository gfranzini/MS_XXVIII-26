   <div n="Folio 12v" type="edition">
    <ab>
     <lb xml:id="Folio12v.1" n="1"/><supplied reason="lost">POR</supplied>RO SI LITTERAE SACRAE MAXIMEQUE 
     <lb xml:id="Folio12v.2" n="2"/>UERACES ITA DICUNT IN PRINCIPIO FECISSE
     <lb xml:id="Folio12v.3" n="3"/><choice><abbr>D&#772;M&#772;</abbr><expan>D<ex>EU</ex>M</expan></choice> CAELUM ET TERRAM UT NIHIL ANTEA <w part="I">FE</w>
     <lb xml:id="Folio12v.4" n="4"/><w part="F">CISSE</w> INTELLEGATUR QUIA HOC POTIUS IN
     <lb xml:id="Folio12v.5" n="5"/>PRINCIPIO FECISSE DICERETUR SI QUID <w part="I">FE</w>
     <lb xml:id="Folio12v.6" n="6"/><w part="F">CISSET</w> ANTE CETERA CUNCTA QUAE FECIT <w part="I">PRO</w>
     <note type="scholion" place="margin"><choice><abbr>qd&#772;</abbr><expan>q<ex>uo</ex>d</expan></choice> ...sit mundu.... sed.....<!--illegible--></note> <lb xml:id="Folio12v.7" n="7"/><w part="F">CUL</w> DUBIO NON EST MUNDUS FACTUS IN
     <lb xml:id="Folio12v.8" n="8"/>TEMPORE SED CUM TEMPORE; QUOD <choice><abbr>ENI &#772;</abbr><expan>ENI<ex>M</ex></expan></choice>
     <lb xml:id="Folio12v.9" n="9"/><supplied reason="lost">FIT IN</supplied> TEMPORE ET POST ALIQUOD <supplied reason="lost">FIT</supplied> ET <w part="I">AN</w>
     <lb xml:id="Folio12v.10" n="10"/><w part="F">TE</w> ALIQUOD TEMPUS POST ID QUOD <w part="I">PRAETE</w>
     <lb xml:id="Folio12v.11" n="11"/><supplied reason="lost"><w part="F">RITUM</w></supplied> EST ANTE ID QUOD FUTURUM EST
     <lb xml:id="Folio12v.12" n="12"/>NULLUM AUTEM POSSET ESSE PRAETERITUM 
     <lb xml:id="Folio12v.13" n="13"/>QUIA NULLA ERAT CREATURA CUIUS <choice><abbr>MUTABILIB&#183;</abbr><expan>MUTABILIB<ex>US</ex></expan></choice> 
     <lb xml:id="Folio12v.14" n="14"/><choice><abbr>MOTIB&#183;</abbr><expan>MOTIB<ex>US</ex></expan></choice> AGERETUR CUM TEMPORE AUTEM
     <lb xml:id="Folio12v.15" n="15"/>FACTUS EST MUNDUS SI IN EIUS CONDITIONE
     <lb xml:id="Folio12v.16" n="16"/>FACTUS EST MUTABILIS MOTUS SICUT <w part="I">UIDE</w>
     <lb xml:id="Folio12v.17" n="17"/><w part="F">TUR</w> SE HABERE ETIAM ORDO ILLE <choice><abbr>PRIMORU &#772;</abbr><expan>PRIMORU<ex>M</ex></expan></choice>
     <lb xml:id="Folio12v.18" n="18"/>SEX UEL SEPTEM DIERUM IN <choice><abbr>QUIB&#183;</abbr><expan>QUIB<ex>US</ex></expan></choice> ET MANE
     <lb xml:id="Folio12v.19" n="19"/>ET <supplied reason="lost">UESPERA</supplied> NOMINANTUR <add place="above">QUI</add> DONEC OMNIA <app><rdg wit="#CSEL40">NOMINANTUR DONEC OMNIA</rdg></app>
     <lb xml:id="Folio12v.20" n="20"/><supplied reason="lost">QUAE HIS DIEBIUS</supplied> <choice><abbr>DS&#772;</abbr><expan>D<ex>EU</ex>S</expan></choice> FECIT SEXTO PERFICIANTUR
     <lb xml:id="Folio12v.21" n="21"/>DIE SEPTIMOQU<del rend="strikethrough">A</del>E IN MAGNO MYSTERIO 
     <note type="scholion" place="margin">R&#772; <choice><abbr>qd&#772;</abbr><expan>q<ex>uo</ex>d</expan></choice> impossibile...<!--illegible--></note> 
     <lb xml:id="Folio12v.22" n="22"/><choice><abbr>DI&#772;</abbr><expan>D<ex>E</ex>I</expan></choice> UACATIO COMMENDETUR QUI DIES CUI<hi rend="ligature">US</hi>
     <lb xml:id="Folio12v.23" n="23"/>MODI SINT AUT PERDIFFICILE NOBIS AUT <choice><abbr>ETIA &#772;</abbr><expan>ETIA<ex>M</ex></expan></choice>
     <lb xml:id="Folio12v.24" n="24"/>INPOSSIBILE EST COGITARE QUANTO MAGIS
     <lb xml:id="Folio12v.25" n="25"/>DICERE; <note type="scholion" subtype="chapter" n="7">V&#772;I&#772;I&#772;</note> UIDEMUS QUIPPE ISTOS DIES <w part="I">NO</w>
     <lb xml:id="Folio12v.26" n="26"/><w part="F">TOS</w> NON HABERE UERSPERAM NISI DE SOLIS
     <lb xml:id="Folio12v.27" n="27"/>OCCASU NEC MANE NISI DE SOLIS EXORTU; 
     <lb xml:id="Folio12v.28" n="28"/>ILLORUM AUTEM PRIORES TRES DIES SINE <w part="I">SO</w>
     <lb xml:id="Folio12v.29" n="29"/><w part="F">LE</w> PERACTIS SUNT QUI DIE QUARTO FACTUS
     <lb xml:id="Folio12v.30" n="30"/>REFERTUR ET PRIMITUS QUIDEM LUX <w part="I">UER</w>
    </ab> 
